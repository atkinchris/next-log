{
  "name": "next-logger",
  "version": "3.0.1",
  "description": "JSON logging patcher for Next.js",
  "main": "index.js",
  "types": "./types/index.js",
  "scripts": {
    "format:all": "prettier --write .",
    "lint": "eslint .",
    "prepare": "git config --local core.eol lf && git config --local core.autocrlf false && git config --local blame.ignoreRevsFile .git-blame-ignore-revs || echo 'git not available, skipping git config step'",
    "prepublish": "npm run generate-type-defs",
    "test": "jest --runInBand",
    "type-check": "tsc --pretty",
    "generate-type-defs": "tsc --pretty"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/atkinchris/next-logger.git"
  },
  "keywords": [
    "next.js",
    "next",
    "logging",
    "logger",
    "log",
    "json"
  ],
  "author": "Chris Atkin <atkinchris@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/atkinchris/next-logger/issues"
  },
  "homepage": "https://github.com/atkinchris/next-logger#readme",
  "peerDependencies": {
    "next": ">=9.0.0"
  },
  "devDependencies": {
    "@tsconfig/node16-strictest": "^1.0.0",
    "@types/node": "^16.x.x",
    "@types/prettier": "^2.7.1",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "next": "^12.2.0",
    "prettier": "^2.7.1",
    "testcontainers": "^8.11.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "cosmiconfig": "^7.0.1",
    "pino": "^7.10.0"
  },
  "jest": {
    "resetMocks": true,
    "resetModules": true,
    "errorOnDeprecated": true
  },
  "volta": {
    "node": "16.14.2",
    "npm": "8.5.0"
  }
}
